{
	"name": "RDW_Import_RAW",
	"properties": {
		"activities": [
			{
				"name": "Fetch RDW CSV",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "MergeFiles"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "RDW_Raw_CSV",
						"type": "DatasetReference",
						"parameters": {
							"object": {
								"value": "@pipeline().parameters.object",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"object": {
				"type": "string",
				"defaultValue": "m9d7-ebf2.json"
			},
			"XAppToken": {
				"type": "string",
				"defaultValue": "miF09EtQWnRrBx3eEVcQ2liRp"
			}
		},
		"folder": {
			"name": "RDW"
		},
		"annotations": []
	}
}