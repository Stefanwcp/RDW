{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-RDW-dev"
		},
		"ADLS_RDW_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_RDW'"
		},
		"ADLS_RDW_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakestoragegenrdwdev.dfs.core.windows.net/"
		},
		"HTTP_RDW_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://opendata.rdw.nl/resource/"
		},
		"LS_AKV_dev_RDW_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://RDW-DEV.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-RDW-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_RDW_accountKey": {
						"value": "[parameters('ADLS_RDW_accountKey')]"
					},
					"ADLS_RDW_properties_typeProperties_url": {
						"value": "[parameters('ADLS_RDW_properties_typeProperties_url')]"
					},
					"HTTP_RDW_properties_typeProperties_url": {
						"value": "[parameters('HTTP_RDW_properties_typeProperties_url')]"
					},
					"LS_AKV_dev_RDW_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_AKV_dev_RDW_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}